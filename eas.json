{
  "cli": {
    "version": ">= 7.8.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
     "preview": {
      "distribution": "internal",
      "channel" : "preview"
    },
    "production": {
      "channel" : "production",
      "env": {
        "EAS_BUILD_DISABLE_NPM_CACHE": "1" 
      }
    }
  },
  "submit": {
    "production": {}
  }
}
